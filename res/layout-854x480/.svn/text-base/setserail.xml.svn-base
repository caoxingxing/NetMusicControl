<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="@drawable/background">
	<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_alignParentTop="true"
		android:layout_marginTop="20dip" android:text="设置" android:textColor="#ff000000"
		android:layout_centerHorizontal="true" android:textSize="30dip"></TextView>
	
	<EditText android:layout_centerHorizontal="true"
		android:layout_centerVertical="true" android:layout_height="50dip" 
		android:layout_width="180dip" android:maxLength="16" android:hint="192.168.1.11"
		android:id="@+id/edit_gatway_ip" android:digits="1234567890."></EditText>
	<TextView android:id="@+id/gateway_ip_label" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_toLeftOf="@+id/edit_gatway_ip"
		android:layout_alignTop="@+id/edit_gatway_ip" android:text="网关IP："
		android:textColor="#ff000000" android:textSize="25dip"></TextView>
		
	<EditText android:digits="1234567890abcdef"
		android:layout_centerHorizontal="true" android:layout_above="@+id/edit_gatway_ip"
		android:layout_height="50dip" android:layout_width="180dip"
		android:maxLength="8" android:hint="请输入序列号"
		android:id="@+id/edit_serial"></EditText>
	<TextView android:id="@+id/textView2" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_toLeftOf="@+id/edit_serial"
		android:layout_alignTop="@+id/edit_serial" android:text="序列号："
		android:textColor="#ff000000" android:textSize="25dip"></TextView>
		
	<EditText android:layout_centerHorizontal="true" android:layout_below="@+id/edit_gatway_ip"
		android:layout_height="50dip" android:layout_width="180dip"
		android:maxLength="4" android:hint="请输入地址码"
		android:id="@+id/edit_power_addr" android:digits="1234567890abcdef"></EditText>
	<TextView android:id="@+id/textView4" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_toLeftOf="@+id/edit_power_addr"
		android:layout_alignTop="@+id/edit_power_addr" android:text="地址码："
		android:textColor="#ff000000" android:textSize="25dip"></TextView>
		
	<EditText android:layout_centerHorizontal="true" android:layout_below="@+id/edit_power_addr"
			android:layout_height="50dip" android:layout_width="180dip"
			android:maxLength="4" android:hint="请输入用户码"
			android:id="@+id/edit_usercode_addr" android:digits="1234567890abcdef"></EditText>
	<TextView android:id="@+id/textView5" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_toLeftOf="@+id/edit_usercode_addr"
		android:layout_alignTop="@+id/edit_usercode_addr" android:text="用户码"
		android:textColor="#ff000000" android:textSize="25dip"></TextView>
		
	<Button android:layout_height="wrap_content"
		android:layout_width="100dip" android:layout_alignParentBottom="true"
		android:id="@+id/btn_ok" android:text="@string/confirm_str"
		android:layout_centerHorizontal="true"></Button>
</RelativeLayout>
