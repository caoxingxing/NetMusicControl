option java_package = "sungeo.netmusic.protocol";  
option java_outer_classname = "AlbumSendPackage"; 
message UserDevAlbum {
	message AlbumPackage {
		optional 	int32 	album_id 		= 1;
		optional 	string 	album_num	= 2;
		optional 	string 	song_id 		= 3;
		optional 	int32 	song_count 	= 4;
		optional 	string 	dev_serial 	= 5;
	}
	repeated AlbumPackage albumPak = 1;
}